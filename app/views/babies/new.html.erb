<h1>Babies#new</h1>
<%= simple_form_for [@baby] do |f| %>
  <%= f.input :first_name %>
  <%= f.input :date_of_birth %>
  <%= f.input :category %>
  <%= f.input :bio %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>

  <%= f.submit "Add a baby", class: "btn btn-primary" %>

<% end %>
